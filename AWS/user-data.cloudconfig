#cloud-config
repo_update: true

packages:
 - g++
 - git
 - libjack-jackd2-dev
 - qt5-default

runcmd:
 - [ git, clone, "https://github.com/AronVietti/jamulus.git" ]
 - [ sh, -c, "cd jamulus; git fetch && git checkout HealthCheck && qmake \"CONFIG+=nosound\" Jamulus.pro && make clean && make"]
 - [ sudo, chrt, 99, ionice, -c1, nice, -n, -20, /jamulus/Jamulus, -s, -n, -p, 22124 ]
